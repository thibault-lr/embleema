    "cypress-vite": "^1.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.2",
    "vite": "^5.1.5",
    "vitest": "^1.3.1"
